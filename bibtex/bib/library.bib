shorttitle = {Cognitive Informatics {\&} Cognitive Computing (ICCI*},
title = {{The T-Sat1 Nanosatellite Team of Teams}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=6311177},
