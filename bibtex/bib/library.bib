@article{Huang1994,
abstract = {JPL/NASA is currently developing microspacecraft systems for future deep space applications. One fo the frequency bands being investigated for microspacecraft is the Ka-band (32 GHz), which can be used with smaller equipment and provides a larger bandwidth. This article describes the successfull development of a circularly polarized microstrip array with 28 dBic of gain at 32 GHz. This antenna, which is thin, flat, and small, can be surface-ounted onto the microspacecraft and, hence, takes very little volume and mass of the spacecraft. The challenges in developing this antenna are minimized the antenna's insertion loss and maintaining a reasonable frequency bandwidth.},
author = {Huang, John},
file = {:home/cuttin/archive/mendeley/The Telecommunications and Data Acquisition Progress Report/Huang - 1994 - A 32-GHz Microstrip Array Antenna for Microspacecraft Application.pdf:pdf},
journal = {The Telecommunications and Data Acquisition Progress Report},
number = {42-116},
pages = {190--203},
title = {{A 32-GHz Microstrip Array Antenna for Microspacecraft Application}},
url = {http://ipnpr.jpl.nasa.gov/progress{\_}report/42-116/116p.pdf},
year = {1994}
}
