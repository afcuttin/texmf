month = {apr},
pages = {2416--2424},
publisher = {IEEE},
shorttitle = {Computer Communications (INFOCOM), 2015 IEEE Confe},
title = {{Steering with eyes closed: Mm-Wave beam steering without in-band measurement}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=7218630},
year = {2015}
}
