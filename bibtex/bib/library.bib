author = {Schaire, Scott H. and Shaw, Harry and Altunc, Serhat and Bussey, George and Celeste, Peter and Kegege, Obadiah and Wong, Yen and Zhang, Yuwen and Patel, Chitra and Raphael, David and Burke, Jacob and Cooper, La Vida and Schier, James and Horne, William and Pierce, David},
booktitle = {14th International Conference on Space Operations},
