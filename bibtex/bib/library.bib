@inproceedings{Elbrecht2011,
abstract = {A new concept for increased power extraction for CubeSats is proposed, which is used for HeidelSat. This new concept contains eight expandable panels capable to host additional solar cells for power generation. It is shown how the eight expandable panels are mounted and how they are released after the Launch. Through simulation, the increased power capacity, in comparison to CubeSats without additional panels, is shown. Furthermore, this work shows a maximum power point tracking schema which is used to extract the maximum available power from solar cells. A block diagram for passing the generated power to batteries is also shown as well as a block diagram for voltage conversion and power management that distributes electricity to other systems controlled by the power management system. Also, a discussion about different battery types as well as their advantages and disadvantages and the redundancy of the power system is included. Furthermore the principle of the distributed power management of HeidelSat is explained and the communication system of the power management as well as the monitoring system of the housekeeping data.},
author = {Elbrecht, Alexander and Dech, Sascha and Gottscheber, Achim},
booktitle = {1st IAA Conference on University Satellites Missions and CubeSat Workshop in Europe},
keywords = {batteries,cubesat,expandable panels,increased power extraction,mppt,power management,solar cells},
