publisher = {Elsevier},
title = {{Super-capacitor energy storage for micro-satellites: Feasibility and potential mission applications}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0094576512004857 http://dx.doi.org/10.1016/j.actaastro.2012.12.005},
volume = {85},
