issn = {0163-6804},
journal = {IEEE Communications Magazine},
keywords = {ALOHA approach,ALOHA system,Access protocols,Bipartite graph,Channel coding,Decoding,Receivers,Silicon carbide,Throughput,access protocols,base station,coded random access,coded slotted aloha,codes,coding theory,collision model,erasure-correcting code,interference suppression,machine-to-machine communications,random access,random access protocols,receiver,review,successive interference cancellation,transmission recovery process},
mendeley-tags = {coded random access,coded slotted aloha,random access,review},
month = {jun},
number = {6},
pages = {144--150},
shorttitle = {Communications Magazine, IEEE},
title = {{Coded random access: applying codes on graphs to design random access protocols}},
