journal = {Acta Astronautica},
keywords = {Hands-on education,Space standard,University satellite,download},
mendeley-tags = {download},
title = {{A space standards application to university-class microsatellites: The UNISAT experience}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0094576509005700},
year = {2009}
}
