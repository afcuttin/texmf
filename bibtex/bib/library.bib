isbn = {978-1-4673-6305-1},
keywords = {ALOHA-type protocols,Decoding,E-IRSA protocol,Iterative decoding,MAC frame lengths,MAC layer,Protocols,Receivers,Signal to noise ratio,Silicon carbide,Throughput,access protocols,active users,bipartite graphs,capture,capture effect,channel capacity,collided packets,collision resolution,distributed operation,e-irsa,interference cancellation,interference suppression,intra-slot SIC,intra-slot interference cancellation,irregular repetition slotted ALOHA,irsa,iterative decoding,iterative physical layer decoding processing,medium access control layer,radio networks,random access protocols,shared channel,sic,signal processing,signal processing techniques,slot-level,slotted ALOHA,successive interference cancellation,successive interference cancellation processing,uncoordinated multiple random access,wireless networks},
mendeley-tags = {capture,e-irsa,interference cancellation,irsa,sic},
